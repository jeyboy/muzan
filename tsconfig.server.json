{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "Node",
    "module": "es6",
    "outDir": "build-server",
    "downlevelIteration": true,
    "noEmit": true,
    "verbatimModuleSyntax": false,
    "target": "es5", 
  },
  "include": ["server"],
  "exclude": [
    "node_modules"
  ]
}
