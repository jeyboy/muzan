{
  "name": "muzan",
  "description": "My Music Collection",
  "type": "module",
  "version": "0.0.1",
  "author": "JB",
  "scripts": {
    "start": "cross-env NODE_ENV=production node build-server/index.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "npm run build:client && npm run build:server",
    "deploy": "vercel --prod",
    "dev:server": "nodemon",
    "dev:client": "cross-env PORT=5000 astro dev",
    "build:server": "tsc --project tsconfig.server.json",
    "build:client": "astro build",
    "cleanup": "rm -rf build/api",
    "postbuild": "react-snap; npm run cleanup",
    "astart": "astro dev",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/svelte": "^4.0.0",
    "@astrojs/tailwind": "^5.0.0",
    "astro": "^3.0.7",
    "astro-icon": "^0.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "svelte": "^4.0.0",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.7",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
